{
  "name": "portal-taco",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && dotenv -e .env.development -- npm run prisma:migrate:deploy && npm run prisma:generate && npm run prisma:seed && next dev",
    "build": "npm run prisma:migrate:deploy && npm run prisma:generate && npm run prisma:seed && next build",
    "dev:next": "next dev",
    "start": "next start",
    "lint": "next lint",
    "services:up": "docker compose -p portal-taco -f infra/compose.yaml up -d",
    "services:stop": "docker compose -p portal-taco -f infra/compose.yaml stop",
    "services:down": "docker compose -p portal-taco -f infra/compose.yaml down",
    "prisma:seed": "ts-node -r tsconfig-paths/register --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" infra/prisma/seed.ts",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema=infra/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=infra/prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env.development  -- npx prisma studio --schema=infra/prisma/schema.prisma",
    "prisma:seed:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" infra/prisma/seed.ts",
    "prisma:seedTest": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" infra/prisma/seedTest.ts",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --schema=infra/prisma/schema.prisma",
    "prisma:migrate:deploy:dev": "dotenv -e .env.development -- npx prisma migrate deploy --schema=infra/prisma/schema.prisma",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate --schema=infra/prisma/schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@monaco-editor/react": "^4.6.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/js-cookie": "^3.0.6",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^11.18.0",
    "jose": "^6.0.8",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.471.1",
    "next": "14.2.23",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.54.2",
    "react-remark": "^2.1.0",
    "react-resizable-panels": "^2.1.7",
    "resend": "^4.1.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.23",
    "postcss": "^8",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
